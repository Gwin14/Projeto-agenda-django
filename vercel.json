{
    "builds": [
      {
        "src": "install_pg.sh",
        "use": "@vercel/static-build"
      },
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],
    "scripts": {
      "build": "./install_pg.sh && pip install --disable-pip-version-check --target . --upgrade -r requirements.txt"
    }
  }
  